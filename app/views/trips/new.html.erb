<h1>New Trip</h1>

<%=form_for @trip do |f|%>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :date %>
  <%= f.date_field :date, "data-provide" => 'datepicker'%>
  
  <a class = "waves-effect waves-light btn" ><%= f.submit %></a>
  <% end %>
