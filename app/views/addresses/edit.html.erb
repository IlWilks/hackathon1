<h1>Editing <%=@location.name%> Address</h1>

<%=form_for [@location, @address] do |f|%>
  <%= f.label :street %>
  <%= f.text_field :street%>

  <%= f.label :city %>
  <%= f.text_field :city %>
  
  <%= f.label :state%>
  <%= f.text_field :state %>
  
  <%= f.label :zipcode %>
  <%= f.text_field :zipcode %>

  <%= f.submit %>
  <% end %>
